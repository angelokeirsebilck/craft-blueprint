{% extends "_layout/_base.twig" %}

{% set entry = craft.entries()
   .slug('404').one() %}

{% block content %}
    {% set content = entry.fieldContentBuilder.all() %}

    {% for block in content %}
        {% include "/_blocks/_#{block.type.handle|replace('type','')|lower}.twig" with {
                "block": block
        } %}
    {% endfor %}
{% endblock %}